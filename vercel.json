{
  "version": 2,
  "builds": [
    { "src": "api/handler.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/handler.js" },
    { "src": "/css/(.*)", "dest": "/css/$1" },
    { "src": "/js/(.*)", "dest": "/js/$1" },
    { "src": "/.*\\..*", "dest": "/$0" },
    { "src": "/(.*)", "dest": "/api/handler.js" }
  ]
}